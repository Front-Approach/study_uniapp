<view class="login-w"><view class="topbg"><image src="../../static/login/login_bg.png" mode></image></view><view class="log-input"><view class="tit"><image src="../../static/login/tit.png" mode></image></view><view class="login"><input class="user-name" type="text" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['user']]]]]}}" value="{{user.userName}}" bindinput="__e"/><input class="user-pass" type="text" password="true" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','passWord','$event',[]],['user']]]]]}}" value="{{user.passWord}}" bindinput="__e"/></view><view class="forget"><text>忘记密码？</text></view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="{{['btn',(isActive)?'cur':'']}}" bindtap="__e"></view></view></view>